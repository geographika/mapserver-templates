        }
      ]
    },
    {
      "anchor": "http://localhost:8080/wms", // this matches the href from the api-catalog
      "service-desc": [
        {
          // the URL to the WMS GetCapabilties request - will only be listed if wms_enable_request GetCapabilities
          // is enabled (or "ows_enable_request" "*"
          "href": "http://localhost:8080/wms/?version=1.0.0&request=GetCapabilities&service=WMS",
          // the MAP NAME
          "name": "WMS_DEMO",
          // the wms_title (or ows_title)
          "title": "MapServer WMS Demo Map",
          // hard-coded MIME type of the response
          "type": "text/xml", // or text/xml; charset=UTF-8 ?
          // a hard-coded type to allow grouping or filtering or URLs in a template
          "service": "WMS",
          // a hard-coded description for WMS GetCapabilities
          "description": "The GetCapabilities URL for version 1.0.0"
